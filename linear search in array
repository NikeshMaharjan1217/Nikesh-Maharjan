#include<stdio.h>
#include<conio.h>
int main()
{
	int i,x,n=6,flag=0;
	int a[]={5,15,28,33,22,1};
	printf("Enter the value:");
	scanf("%d",&x);
	for(i=0;i<n;i++)
	{
		if(a[i]==x)
		{
		printf("%d is found in %d",x,i+1);
		flag=1;
		break;
		} 
	
    }
	if(flag==0)
	{
	printf("%d is not in array",x);
    }
	getch();
}
